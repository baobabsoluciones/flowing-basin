{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "dams": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "order": {
                        "type": "integer"
                    },
                    "vol_min": {
                        "type": "number"
                    },
                    "vol_max": {
                        "type": "number"
                    },
                    "relevant_lags": {
                        "type": "array",
                        "items": {
                            "type": "integer"
                        }
                    },
                    "max_flow_points": {
                        "type": "array",
                        "items": {
                            "type": "array",
                            "items": {
                                "type": "integer"
                            }
                        }
                    },
                    "initial_vol": {
                        "type": "number"
                    },
                    "initial_lags": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    },
                    "unregulated_flow": {
                        "type": "number"
                    }
                },
                "required": [
                    "id",
                    "initial_lags",
                    "initial_vol",
                    "max_flow_points",
                    "order",
                    "relevant_lags",
                    "unregulated_flow",
                    "vol_max",
                    "vol_min"
                ]
            }
        },
        "time_step_minutes": {
            "type": "integer"
        },
        "incoming_flows": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "energy_prices": {
            "type": "array",
            "items": {
                "type": "number"
            }
        }
    },
    "required": [
        "dams",
        "energy_prices",
        "incoming_flows",
        "time_step_minutes"
    ]
}